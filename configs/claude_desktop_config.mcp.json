{
  "mcpServers": {
    "support-db": {
      "command": "uv",
      "args": [
        "run",
        "python",
        "/PATH/TO/neo4j-agno-agent/server.py"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "password",
        "GEMINI_API_KEY": "YOUR_GEMINI_API_KEY"
      }
    }
  }
}
